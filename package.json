{
  "name": "closure-stylesheets",
  "version": "0.0.0",
  "description": "A Node API to Closure Stylesheets.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "yarn test -e test-build",
    "lint": "eslint .",
    "lib": "depack src/depack -o compile/closure-stylesheets.js -a -c -p --source_map_include_content",
    "doc": "doc -o README.md -d -a -n _artdeco",
    "b": "alamode src -o build -s",
    "d": "yarn-s template d1 externs",
    "template": "typal compile/template.js -T compile/index.js -u -t types",
    "d1": "typal compile/index.js types/index.js -u -t types/index.xml",
    "externs": "typal types/externs.js -e",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "build",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/closure-stylesheets.git"
  },
  "typedefs": "typedefs.json",
  "keywords": [
    "closure-stylesheets"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/artdecocode/closure-stylesheets/issues"
  },
  "homepage": "https://github.com/artdecocode/closure-stylesheets#readme",
  "devDependencies": {
    "alamode": "^3.5.2",
    "depack": "^1.1.2",
    "documentary": "^1.37.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "spawncommand": "^2.2.0",
    "typal": "^1.26.2",
    "yarn-s": "1.2.3",
    "zoroaster": "^4.3.0"
  }
}
